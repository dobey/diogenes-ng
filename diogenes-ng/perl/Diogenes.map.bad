my $babel_latex = sub
{
	my $ref = shift; 	
	$$ref =~ s/{\d*/</g;
	$$ref =~ s/}\d*/>/g;
	$$ref =~ s/\[1/(/g;
	$$ref =~ s/\]1/)/g;
	$$ref =~ s/\[[23]/¬¬textless{}/g;
	$$ref =~ s/\][23]/¬¬textgreater{}/g;
	$$ref =~ s/\[\d*/{[}/g;
	$$ref =~ s/\]\d*/{]}/g;
	$$ref =~ s/\<\d*/¬¬textless{}/g;
	$$ref =~ s/\>\d*/¬¬textgreater{}/g;
	$$ref =~ s/"1/¬¬quotedblbase{}/g;
	$$ref =~ s/"2/¬¬textquotedblleft{}/g;
	$$ref =~ s/"4/¬¬quotesinglbase{}/g;
	$$ref =~ s/"5/¬¬textquoteleft{}/g;
	$$ref =~ s#([\s\n~])\"6#$1¬¬guillemotleft{}#g;
	$$ref =~ s#\"6#¬¬guillemotright{}#g;
	$$ref =~ s#([\s\n~])\"7#$1¬¬guilsinglleft{}#g;
	$$ref =~ s#\"7#¬¬guilsinglright{}#g;
	#$$ref =~ s/"/¬¬"{}/g;
	$$ref =~ s#([\s\n~])\"3?(\"3)?#$1¬¬textquoteleft{}#g;
	$$ref =~ s#\"3?(\"3)?#¬¬textquoteright{}#g;
	$$ref =~ s#`##g;
	$$ref =~ s#([a-zA-Z][123/\\|+)(=]*)\?#¬¬d{$1}#g;
	$$ref =~ s#([/\\|+)(=]+)\?#¬¬d{$1}#g;
	$$ref =~ s/\?/¬¬textlatin{?}/g;
	$$ref =~ s/!/./g;
	$$ref =~ s/#\d*/*/g;
	$$ref =~ s/%13(?!\d)/¬¬ddag{}/g;
	$$ref =~ s/%14(?!\d)/¬¬P{}/g;
	$$ref =~ s/%18(?!\d)/\'\'/g;
	$$ref =~ s/%19(?!\d)/-/g;
	$$ref =~ s/%1(?!\d)/¬¬textlatin{?}/g;
	$$ref =~ s/%3(?!\d)/\//g;
	$$ref =~ s/%4(?!\d)/!/g;
	$$ref =~ s/%5(?!\d)/|/g;
	$$ref =~ s/%6(?!\d)/=/g;
	$$ref =~ s/%7(?!\d)/+/g;
	$$ref =~ s/%8(?!\d)/%/g;
	$$ref =~ s/%9(?!\d)/&/g;
	$$ref =~ s/%\d*/*/g;
	$$ref =~ s/%/¬¬dag{}/g;
	$$ref =~ s/@[12]/\n/g;
#	produces spurious latin text in a modern greek environment
#	$$ref =~ s/\$(?!\d)/¬¬latintext{}/g;
	$$ref =~ s/\$\d?[16](?!\d)/¬¬bf{}/g;
	$$ref =~ s/\$\d?3(?!\d)/¬¬it{}/g;
#	$$ref =~ s/\$9(?!\d)/¬¬latintext{}/g;

	$$ref =~ s#@\d+#\n#g; 
	$$ref =~ s#@# #g;
	$$ref =~ s/(\ \ +)/my $l=length($1);'{'.('¬¬ 'x$l) .'}'/ge; #only multiple spaces

};
$Diogenes::Base::encoding{Babel_7}{pre_match} = $babel_latex;

my $babel_latex_post = sub 
{
	my $ref = shift; 	
	$$ref =~ s/¬¬/\\/g; # Unescape the backslashes
	$$ref =~ s/\n/\\ \\\\\n/g; # Line-breaks
	$$ref = "\\selectlanguage{polutonikogreek}\n" . $$ref . "\n\\latintext";

};
$Diogenes::Base::encoding{Babel_7}{post_match} = $babel_latex_post; 

##################################################################

# Another option is to define an encoding by passing a reference
# to Perl code that will do the translation directly.  This will
# be much quicker than using a character map, as the other examples
# in this file do, but it is only appropriate for very simple
# transformations.

##################################################################



# SPIonic is a public domain TrueType font that keeps very close
# to the BETA encoding.  This code does not take advantage of its
# distinction between wide and narrow letters.

$Diogenes::Base::encoding{SPIonic}{sub} = sub 
{
	my $ref = shift; 		# The text is passed as a reference to a string
							# (Note double $$ in subsequent lines)

	$$ref =~ tr/A-Z/a-z/;   # Make everything lower case
	$$ref =~ s/s1/s/g;		# Normal sigma
	$$ref =~ s/s2/j/g;		# Final sigma is `j' in this encoding
	$$ref =~ s/s3/s/g;      # Lunate sigma is unsupported, so use normal sigma
	$$ref =~ s#`#§#g;		# The ` char in BETA is a null separator -- ascii output 
							# format will not remove these, since they are
							# relevant to the Ibycus encoding -- so we change them
							# to something that will get removed.
							
	$$ref =~ s/\*(\W*)(\w)/$1\u$2/g;	# Remove *, uppercasing following letter;

};							# Note trailing semicolon.

# Some styles of Greek encoding cannot be intermixed with Latin
# letters without confusion -- in other words, they remap the ascii or
# iso_8859_1 ranges (<255).  It may help some markup formats to give a 
# font name to be associated with this encoding in order to distinguish
# it from Latin text.
$Diogenes::Base::encoding{SPIonic}{font_name} = 'SPIonic';


# Standard installations of TeX such as teTeX come with the LaTeX Babel
# package and the cbgreek fonts.  This uses a different polytonic
# encoding to Ibycus, which is the native format used by Diogenes when
# generating LaTeX files.  To use this encoding say in your LaTeX file:
# \usepackage[greek]{babel}
# \languageattribute{greek}{polutoniko}

$Diogenes::Base::encoding{Babel}{pre_match} = sub
{
	my $ref = shift;
	$$ref =~ tr/A-Z/a-z/;
	$$ref =~ s/q/j/g;
	$$ref =~ s/x/q/g;
	$$ref =~ s/c/x/g;
	$$ref =~ s/s1/s/g;
	$$ref =~ s/s2/c/g;
	$$ref =~ s/\*(\W*)(\w)/$1\u$2/g; # Upper-case

};
$Diogenes::Base::encoding{Babel}{sub} = $babel_latex;
$Diogenes::Base::encoding{Babel}{post_match} = sub
{
	my $ref = shift;
	$$ref =~ s/\'/''/g;   # coronis is ''
	$$ref =~ s/(\w)\)/>$1/g;
	$$ref =~ s/(\w)\(/<$1/g;
	$$ref =~ s#(\w)\/#'$1#g;  # Accent comes before the letter!
	$$ref =~ s#(\w)\\#\`$1#g;
	$$ref =~ s/(\w)\=/~$1/g;
	$$ref =~ s/(\w)\+/"$1/g;
	$$ref =~ s/\+/"/g;  # Accents before upper-case
	$$ref =~ s/\)/>/g;
	$$ref =~ s/\(/</g;
	$$ref =~ s/\//'/g;  
	$$ref =~ s/\\/`/g;
	$$ref =~ s/\=/~/g;
	$$ref =~ s/\;/?/g;
	$$ref =~ s/\:/;/g;
	$babel_latex_post->($ref);
};


$Diogenes::Base::encoding{SGreek}{sub} = sub 
{
	my $ref = shift;
	$$ref =~ tr/A-Z/a-z/;
	$$ref =~ s/s[123]/s/g;
	$$ref =~ s#(?<!\*)s(?![a-z)(|/\\=+])#j#g;	# Final sigma
	$$ref =~ s/\*(\W*)(\w)/$1\u$2/g;
};

$Diogenes::Base::encoding{SGreek}{font_name} = 'Sgreek Fixed';

