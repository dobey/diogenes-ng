# This subroutine is called after the match to add underdots, etc. etc.
# All the nice characters that Unicode gives us, which otherwise are
# left up to the formatting layer.

my $UTF_8_post = sub
{
	my $ref = shift;
	$$ref =~ s/\?/Ì£/g;        #underdots
	$$ref =~ s/\%(?!\d)/â /g;  #dagger
	$$ref =~ s/\%1(?!\d)/?/g;   #roman question mark
	$$ref =~ s/\%12(?!\d)/â»/g; #asteriskos
	$$ref =~ s/\%13(?!\d)/â¡/g; #double dagger
	$$ref =~ s/\%14(?!\d)/Â§/g; #section sign
	
	$$ref =~ s/\%16(?!\d)/Â¦/g; #broken vertical
	$$ref =~ s/\%17(?!\d)/â/g;  #double vertical
	$$ref =~ s/\%19(?!\d)/-/g;  #hyphen
	# combining accents
	$$ref =~ s/\%20(?!\d)/Ì/g;  #acute
	$$ref =~ s/\%21(?!\d)/Ì/g;  #grave
	$$ref =~ s/\%22(?!\d)/Ì/g;  #circumflex
	$$ref =~ s/\%2[38](?!\d)/Ì/g;  #diaresis
	$$ref =~ s/\%24(?!\d)/Ì/g;  #tilde
	$$ref =~ s/\%25(?!\d)/Ì§/g;   #cedilla
	$$ref =~ s/\%26(?!\d)/Ì/g;  #macron
	$$ref =~ s/\%27(?!\d)/Ì/g;  #breve
	$$ref =~ s/\%29(?!\d)/Ì£/g;         #(double) underdot
	# stand-alone accents
	$$ref =~ s/\%30(?!\d)/á¾½/g;         #lenis
	$$ref =~ s/\%31(?!\d)/á¿¾/g;         #asper
	$$ref =~ s/\%32(?!\d)/á¿½/g;  #oxy
	$$ref =~ s/\%33(?!\d)/á¿¯/g;  #bary
	$$ref =~ s/\%34(?!\d)/á¿/g; #peri
	$$ref =~ s/\%35(?!\d)/á¿/g; #lenis_oxy   	
	$$ref =~ s/\%36(?!\d)/á¿/g; #asper_oxy   	
	$$ref =~ s/\%37(?!\d)/á¿/g; #asper_bary  	
	$$ref =~ s/\%38(?!\d)/á¿/g; #asper_peri  	
	$$ref =~ s/\%39(?!\d)/Â¨/g;   #diaer			
	
	$$ref =~ s/\!/\./g;   #missing letter dot			

};

$Diogenes::Base::encoding{'UTF-8-CX'}{post_match} = $UTF_8_post;
$Diogenes::Base::encoding{'UTF-8-CB'}{post_match} = $UTF_8_post;
